{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-unused-expressions": "warn",
    "no-unused-labels": "warn",
    "jsx-a11y/anchor-is-valid": "warn"
  }
}